{
    "routes": [
      {
        "src": "/login",
        "dest": "/index.html"
      },
      {
        "src": "/signup",
        "dest": "/index.html"
      },
      {
        "src": "/movies/:id",
        "dest": "/index.html"
      },
      {
        "src": "/favorites",
        "dest": "/index.html"
      },
      {
        "src": "/",
        "dest": "/index.html"
      }
    ],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ]
  }
  